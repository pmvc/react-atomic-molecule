{
  "description": "keyframe lib",
  "repository": "react-atomic/react-atomic-molecule",
  "keywords": [
    "reactjs"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.5",
    "react": "^16.x"
  },
  "scripts": {
    "build:cjs": "BABEL_ENV=build babel src -d build",
    "build:es": "BABEL_ENV=es babel src/index.js -o build/index.es.js",
    "copy": "cp package.json.build ./build/package.json && cp README.md ./build/",
    "build": "rm -rf build && npm run build:cjs && npm run build:es && npm run copy && cd build && yarn install",
    "prepublish": "npm run build"
  }
}
