  "scripts": {
    "esm": "yo reshow:update-esm-export",
    "esm:n": "yo reshow:update-esm-export -n",
    "clean": "find ./build ./types -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=cjs babel ui -d build/cjs/ui --ignore /**/__tests__ --root-mode upward",
    "build:es": "BABEL_ENV=es babel ui -d build/es/ui --out-file-extension .mjs --root-mode upward",
    "build:type": "npx -p typescript tsc ui/*.jsx --jsx react-jsx --declaration --allowJs --emitDeclarationOnly --skipLibCheck --declarationDir types",
    "build": "npm run clean && npm run build:cjs && npm run build:es && npm run build:type",
    "mochaFor": "STRICT_MODE=on mocha -r global-jsdom/register",
    "mocha": "npm run mochaFor -- 'build/es/**/__tests__/*.mjs'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/*"
      ]
    }
  },
  "files": [
    "types",
    "build",
    "package.json",
    "README.md"
  ]
